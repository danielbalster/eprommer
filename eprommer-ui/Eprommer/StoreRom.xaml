<Window x:Class="Eprommer.StoreRom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Eprommer"
        mc:Ignorable="d"
        Title="StoreRom" Height="132.5" Width="262.723" WindowStartupLocation="CenterOwner" Icon="logo.ico">
    <Canvas>
        <TextBlock Height="22" Canvas.Left="10" Canvas.Top="10">Name:</TextBlock>
        <TextBox Width="200" Height="22" Name="Name" Canvas.Left="49" Canvas.Top="10" ></TextBox>
        <TextBlock Height="22" Canvas.Left="10" Canvas.Top="37" RenderTransformOrigin="0.654,-0.25" >CRC:</TextBlock>
        <TextBlock Height="22" Name="CRC" Canvas.Left="49" Canvas.Top="37" >00000000</TextBlock>
        <TextBlock Height="22" Name="Size" Canvas.Left="151" Canvas.Top="37" >0</TextBlock>
        <Button Width="50" Height="22" Canvas.Left="140" Canvas.Top="69" IsDefault="True" Click="Button_Click" >Save</Button>
        <Button Width="50" Height="22" Canvas.Left="195" Canvas.Top="69" IsCancel="True" >Cancel</Button>
        <TextBlock Height="22" Canvas.Left="122" Canvas.Top="37" ><Run Text="Size:"/></TextBlock>
    </Canvas>
</Window>
